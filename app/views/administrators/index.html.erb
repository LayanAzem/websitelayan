<p id="notice"><%= notice %></p>

<h1 id="blue-head">Listing Administrators</h1>



<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Access level</th>
      <th>Contact</th>
      
      <th>School</th>
      <th>Email</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administrators.each do |administrator| %>
      <tr>
        <td><%= administrator.firstname %></td>
        <td><%= administrator.lastname %></td>
        <td><%= administrator.access_level %></td>
        <td><%= administrator.contact %></td>
        <td><%= administrator.school_id %></td>
        <td><%= link_to 'Show', administrator %></td>
        <td><%= link_to 'Edit', edit_administrator_path(administrator) %></td>
        <td><%= link_to 'Destroy', administrator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Administrator', new_administrator_path %>
