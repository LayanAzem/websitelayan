<%= simple_form_for(@administrator) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :firstname %>
    <%= f.input :lastname %>
    <%= f.input :access_level %>
    <%= f.input :contact %>
    <%= f.input :school_id %>
    <%= f.input :email %>
    <%= f.input :password%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
